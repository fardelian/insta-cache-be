{
  "name": "insta-cache-be",
  "version": "0.0.1",
  "description": "InstaCache back-end",
  "main": "src/index.ts",
  "scripts": {
    "preinstall": "husky install",
    "validate-package-json": "node ./scripts/validate-package-json.js",
    "lint": "eslint './src/**'",
    "lint:fix": "eslint --fix './src/**'",
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "start": "node ./build/src/index.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "docs": "typedoc",
    "clean": "rm -rf artifacts build node_modules"
  },
  "keywords": [
    "insta-cache",
    "instacache",
    "instagram",
    "cache"
  ],
  "author": "Florin Ardelian <florin@ardelian.ro>",
  "license": "UNLICENSED",
  "devDependencies": {
    "@faker-js/faker": "6.0.0-alpha.7",
    "@types/express": "4.17.13",
    "@types/jest": "27.4.1",
    "@types/node": "17.0.21",
    "@typescript-eslint/eslint-plugin": "5.12.1",
    "@typescript-eslint/parser": "5.12.1",
    "eslint": "8.9.0",
    "eslint-config-google": "0.14.0",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "jest-stare": "2.3.0",
    "nodemon": "2.0.15",
    "ts-jest": "27.1.3",
    "ts-node": "10.5.0",
    "typedoc": "0.22.12",
    "typedoc-plugin-merge-modules": "3.1.0",
    "typescript": "4.5.5"
  },
  "dependencies": {
    "dotenv": "16.0.0",
    "express": "4.17.3",
    "helmet": "5.0.2",
    "http-status-codes": "2.2.0",
    "log4js": "6.4.1"
  }
}
